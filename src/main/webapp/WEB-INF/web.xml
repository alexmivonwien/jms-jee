<web-app xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
                    http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
                    version="3.0">
	
	<!-- JSF mapping -->
	<servlet>
		<servlet-name>Timer Starter Servlet</servlet-name>
		<servlet-class>at.alexander.jms.client.TimerStarterServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	    
	<servlet-mapping>
        <servlet-name>Timer Starter Servlet</servlet-name>
       	<url-pattern>/timerStarter</url-pattern>
    </servlet-mapping>
	
</web-app>
